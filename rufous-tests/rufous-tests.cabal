-- Initial rufous.cabal generated by cabal init.  For further 
-- documentation, see http://haskell.org/cabal/users-guide/

name:                rufous-tests
version:             0.1.0.0
synopsis:            Test suite for rufous
-- description:
license:             BSD3
license-file:        LICENSE
author:              Ben Simner
maintainer:          Ben Simner <benjsimner@gmail.com>
-- category:            Testing
build-type:          Simple
cabal-version:       >=1.10

source-repository head
  type:     git
  location: https://github.com/bensimner/rufous

executable rufous-tests-example-queue
  main-is:             Queue.hs
  build-depends:       base
                       , rufous
  hs-source-dirs:      exe
  default-language:    Haskell2010
  ghc-options:         -Wall

executable rufous-tests-example-list
  main-is:             List.hs
  build-depends:       base
                       , rufous
  hs-source-dirs:      exe
  default-language:    Haskell2010
  ghc-options:         -Wall
